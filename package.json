{
  "name": "auth-server",
  "version": "1.0.0",
  "description": "Authentication service",
  "main": "index.js",
  "scripts": {
    "start": "node src/bin/www",
    "start:dev": "nodemon src/bin/www --ignore __tests__",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:reset": "npx sequelize-cli db:seed:undo:all && npm run seed",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset && cross-env NODE_ENV=test npm run seed:reset",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000"
  },
  "keywords": [],
  "author": "Zuev Roman",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "http://www.github.com"
  },
  "dependencies": {
    "bcrypt": "^3.0.8",
    "config": "^3.2.5",
    "debug": "latest",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "klaw-sync": "^6.0.0",
    "pg": "^7.18.1",
    "sequelize": "^5.21.4",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2"
  }
}
